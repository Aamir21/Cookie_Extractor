{
    "name" : "Cookie Extractor",
    "version" : "1.0",
    
    "description" : "An extension to extract your browser cookies!",
    "permissions": [ "cookies","storage", "tabs", "http://*/*", "https://*/*" , "nativeMessaging"],
    "icons": { "16": "icon2.png", "48": "icon2.png", "128": "icon2.png" },
    "browser_action": {
      "default_title": "Cookie Extractor",
      "default_icon": "icon2.png",
      "default_popup": "index.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2
  }
  